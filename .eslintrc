{
  "globals": {
    "atom": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": 0,
    "no-mixed-requires": 0,
    "no-comma-dangle": 0,
    "space-infix-ops": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "no-trailing-spaces": 0,
    "comma-dangle": 0,
    "strict": 0,
    "no-unused-vars": 1,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "curly": 1,
    "no-path-concat": 1,
    "no-console": 0,
    "eqeqeq": 1
  }
}
